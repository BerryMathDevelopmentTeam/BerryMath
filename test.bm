class node {
public:
	def ctor(v = 0, l = null, r = null) {
		this.v = v;
		this.l = l;
		this.r = r;
	}
	def insert(n) {
		if (n < this.v) {
			if (this.l) {
				this.l.insert(n);
			} else {
				this.l = new node(n);
			}
		} else {
			if (this.r) {
				this.r.insert(n);
			} else {
				this.r = new node(n);
			}
		}
	}
	def sort(arr) {
		if (this.l) { this.l.sort(arr); }
		arr.push(this.v);
		if (this.r) { this.r.sort(arr); }
	}
}

class BinTree {
public:
	def ctor() {
		this.root = null;
	}
	def insert(n) {
		if (this.root == null) {
			this.root = new node(n);
		} else {
			this.root.insert(n);
		}
	}
	def sort() {
		let arr = [];
		if (!this.root) { return arr; }
		this.root.sort(arr);
		return arr;
	}
}

def swap(refer a, refer b) {
    let tmp = a;
    a = b;
    b = tmp;
}

